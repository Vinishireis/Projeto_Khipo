{
  "expo": {
    "name": "Calculadora_Khipo",
    "slug": "Calculadora_Khipo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo_Khipo_Transparente.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-1024-1024.png",
        "backgroundColor": "#55329a"
      },
      "package": "com.vinishireis.Calculadora_Khipo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon-1024-1024.png"
    },
    "splash": {
      "image": "./assets/images/splash-4096-4096.png",
      "resizeMode": "contain",
      "backgroundColor": "#55329a"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-1024-1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#55329a"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2349ce8b-e649-4a31-98e5-ff4efddbd612"
      }
    },
    "owner": "vinishireis"
  }
}
